{
  "name": "salz-web",
  "version": "0.0.1",
  "description": "A web frontend for salz",
  "author": "Lugarun, Mosiman, Japorized",
  "private": true,
  "scripts": {
    "install": "npm install",
    "dev:docker": "NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "BASE_URL=https://salz.life nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/markdownit": "^1.2.6",
    "hotkeys-js": "^3.7.2",
    "nuxt": "^2.10.1",
    "nuxt-buefy": "^0.3.2",
    "pixi-viewport": "^4.2.2",
    "pixi.js": "^5.1.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^2.6.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "vue-jest": "^4.0.0-0"
  }
}
