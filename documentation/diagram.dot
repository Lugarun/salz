digraph ServiceInteraction {
    graph[rankdir=LR]
    node[shape = record] 

    ranker [label="Ranker"]

    api [label="API"]
    frontend [label="Front End"]
    db [label="Database"]
    s3 [label="Object Storage"]
    game [label="Game Engine"]
    mm [label="Match Handler"]
    bh [label="Bot Handler"]

    api -> frontend [dir="both"]
    db -> api [dir="both"]
    s3 -> api [dir="both"]

    db -> ranker
    ranker -> api
    
    s3 -> mm [dir="both"]
    ranker -> mm
    mm -> db

    mm -> game [dir="both"]
    mm -> bh [dir="both"]
}
